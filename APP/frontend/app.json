{
  "expo": {
    "name": "APP",
    "slug": "APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/banner2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/banner2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.nome.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/banner2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "dev.nome.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "800a1757-cb4f-485e-95df-ebb9577fab1e"
      }
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
